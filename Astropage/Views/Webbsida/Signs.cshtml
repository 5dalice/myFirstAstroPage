@model List<astro>
@{
    Layout = "~/Views/Shared/minMall.cshtml";
}
<link href="~/css/minStil.css" rel="stylesheet" />

<!-- <div class="minHeader"></div>  <div class="container"></div> -->

<main class="container">

    <div class="content-view">
        <div class="read-more-content">
            <h1>In this section, you can see the birthdays of some of our users.</h1>
            <form asp-controller="Webbsida" asp-action="LäggaTill" method="get">
                <button type="submit" class="btn btn-primary">Add more</button>
            </form>


            <table class="table table-hover table-primary">
                <thead>
                    <tr>

                        <th scope="col">Name</th>
                        <th scope="col">Birthday</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (astro astro in Model)
                    {
                        <tr>
                            <td>@astro.Name</td>
                            <td>@astro.Date</td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</main>


<!--Footer -->
<div class="container">
    <footer class="d-flex flex-wrap justify-content-between align-items-center py-3 my-4 border-top">
        <div class="col-md-4 d-flex align-items-center">
            <a href="/" class="mb-3 me-2 mb-md-0 text-body-secondary text-decoration-none lh-1">
                <svg class="bi" width="30" height="24"><use xlink:href="#bootstrap" /></svg>
            </a>
            <span class="mb-3 mb-md-0 text-body-secondary">&copy; 2023 Alice Astrology, Inc</span>
        </div>
    </footer>
</div>

